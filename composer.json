{
  "name": "tourze/symfony-routing-auto-loader-bundle",
  "description": "Symfony bundle for automatic routing enhancement and auto-loading",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\RoutingAutoLoaderBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\RoutingAutoLoaderBundle\\Tests\\": "tests"
    }
  }
}